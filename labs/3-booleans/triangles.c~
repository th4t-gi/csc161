 */
 
#include <stdio.h>

int main(void) {

  int a, b, c;

  printf("Please give side a:\n");
  scanf("%d", &a);

   printf("Please give side b:\n");
   scanf("%d", &a);
  
   printf("Please give side c:\n");
   scanf("%d", &a);
  
}
